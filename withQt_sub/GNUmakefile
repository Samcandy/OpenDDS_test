MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: withQt

depend: withQt-depend

REMAINING_TARGETS := $(filter-out all depend,$(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.withQt $(@)

.PHONY: withQt
withQt:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.withQt all

.PHONY: withQt-depend
withQt-depend:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.withQt depend

project_name_list:
	@echo withQt
