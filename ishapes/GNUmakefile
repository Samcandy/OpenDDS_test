MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: ishapes

depend: ishapes-depend

REMAINING_TARGETS := $(filter-out all depend,$(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.ishapes $(@)

.PHONY: ishapes
ishapes:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.ishapes all

.PHONY: ishapes-depend
ishapes-depend:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.ishapes depend

project_name_list:
	@echo ishapes
